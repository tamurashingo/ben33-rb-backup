<h1>イベントリスト</h1>

<table>
  <thead>
    <tr>
      <th>イベント名</th>
      <th>開始日時</th>
      <th>終了日時</th>
      <th>場所</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.start_date %></td>
        <td><%= event.end_date %></td>
        <td><%= event.venue %></td>
      </tr>
    <% end %>
  </tbody>
</table>

